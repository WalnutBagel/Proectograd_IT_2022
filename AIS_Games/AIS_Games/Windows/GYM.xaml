<Window x:Class="AIS_Games.Windows.GYM"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIS_Games"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="White" >
    <Grid Margin="0,0,0,90">
        <Grid.Background>
            <ImageBrush ImageSource="Resources/RickRoll.gif"/>
        </Grid.Background>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Height="110" Margin="0,0,-386,-90">
            <Button x:Name="Start" Click="Start_Click" Content="Start" Height="110" Width="120"/>
            <TextBlock Text="Friday RickRoll Funkin" Height="30" Width="374"/>
        </StackPanel>
    </Grid>
</Window>
